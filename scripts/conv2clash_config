import io
import base64
import json

def main():
    vmess2clash()

def vmess2clash():
    url = "vmess:".strip()
    if url[:8] != "vmess://":
        raise ValueError(f"{url[:8]} != 'vmess://'")
    vmess_dict = json.loads(base64.b64decode(url[8:]).decode('utf-8'))
    clash_dict = {
            "name": vmess_dict['ps'],
            "type": "vmess",
            "server": vmess_dict['add'],
            "port": vmess_dict['port'],
            "cipher": 
            "uuid":
            "alterID":
            "tls":
            "skip-cert-verify":
            "network":
            "ws-opts": {
                "path": 
            }
    }

