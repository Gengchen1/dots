super + Return
	$TERMINAL
super + shift + Return
	alacritty --title ranger -e ranger
super + d
	rofi -show run
	# dmenu_run
super + shift + d
	rofi -show drun -show-icons -modi drun
super + i
	dmenuunicode
super + c
	$BROWSER
super + x
	firefox
super + z
	emacs
super + g
	pcmanfm
super + t
	zeal
super + shift + t
	goldendict
super + p
	vnote
ctrl + shift + a
	flameshot gui
Print
	maim ~/Pictures/pic-full-$(date '+%y%m%d-%H%M-%S').png && notify-send -u low "截取屏幕成功"

# Function keys
XF86AudioMute
	lmc toggle
XF86AudioRaiseVolume
	lmc up
XF86AudioLowerVolume
	lmc down
XF86AudioPlay
	mpc toggle
XF86AudioPrev
	mpc prev
XF86AudioNext
	mpc next
XF86AudioStop
	mpc stop
XF86Tools
	alacritty --title ncmpcpp -e ncmpcpp
XF86HomePage
	alacritty --title ranger -e ranger
shift + Print
	maimpick
