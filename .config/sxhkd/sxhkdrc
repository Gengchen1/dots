super + Return
	$TERMINAL
super + shift + Return
	alacritty --title ranger -e ranger
super + d
	rofi -show run -matching fuzzy
	# dmenu_run
super + w
	rofi -show window -show-icons
super + shift + d
	rofi -show drun -show-icons
super + i
	rofi -modi 'emoji:~/.local/bin/rofiemoji/rofiemoji.sh' -show emoji
	# dmenuunicode
super + shift + i
	rofi -show combi -show-icons
super + c
	$BROWSER
super + shift + x
	emacs
super + x
	code
super + z
	gcolor2
super + g
	pcmanfm
super + t
	zeal
super + p
	vnote
super + shift + m
	rofi -show calc -no-show-match -no-sort
super + m
	goldendict
ctrl + shift + a
	flameshot gui
Print
	maim ~/Pictures/pic-full-$(date '+%y%m%d-%H%M-%S').png && notify-send -u low "截取屏幕成功"

# Function keys
XF86AudioMute
	lmc toggle
XF86AudioRaiseVolume
	lmc up
XF86AudioLowerVolume
	lmc down
XF86AudioPlay
	mpc toggle
XF86AudioPrev
	mpc prev
XF86AudioNext
	mpc next
XF86AudioStop
	mpc stop
XF86Tools
	alacritty --title ncmpcpp -e ncmpcpp
XF86HomePage
	alacritty --title ranger -e ranger
shift + Print
	maimpick
