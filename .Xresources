! -------* Xft配置 *--------!
Xft.dpi:                          96
Xft.antialias:                    true
Xft.hinting:                      true
Xft.rgba:                         rgb
Xft.autohint:                     false
Xft.hintstyle:                    hintslight
Xft.lcdfilter:                    lcddefault

*background:                      #25252c
*foreground:                      #ffffff
*cursorColor:                     #ffffff
*fading:                          8
*fadeColor:                       black
*pointerColorBackground:          #2B2C2B
*pointerColorForeground:          #16A085

! -------* urxvt配置 *--------!
! 实现背景透明需要安装xcompmgr
URxvt*font:                       xft:Source Code Pro:size=12, xft:Source Han Sans CN:size=12, xft:SauceCodePro Nerd Font:size=12
URxvt*depth:                      32
URxvt*background:                 [100]black
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                false
URxvt*cursorUnderline:            false
URxvt*saveLines:                  5000
URxvt*internalBorder:             5
URxvt*geometry:                   85x30
URxvt*perl-ext-common:            default,matcher
URxvt*url-launcher:               /usr/bin/xdg-open
URxvt*matcher.button:             1
URxvt*matcher.rend.0:             Uline Bold fg5
! 实现正常的复制粘贴
URxvt*iso14755:                   false
URxvt*keysym.Shift-Control-V:     eval:paste_clipboard
URxvt*keysym.Shift-Control-C:     eval:selection_to_clipboard

URxvt*color0:                     #181818
URxvt*color1:                     #ab4642
URxvt*color2:                     #a1b56c
URxvt*color3:                     #f7ca88
URxvt*color4:                     #7cafc2
URxvt*color5:                     #ba8baf
URxvt*color6:                     #86c1b9
URxvt*color7:                     #d8d8d8
URxvt*color8:                     #585858
URxvt*color9:                     #ab4642
URxvt*color10:                    #a1b56c
URxvt*color11:                    #f7ca88
URxvt*color12:                    #7cafc2
URxvt*color13:                    #ba8baf
URxvt*color14:                    #86c1b9
URxvt*color15:                    #f8f8f8

